{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "removeComments": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "noImplicitReturns": true,
        "suppressImplicitAnyIndexErrors": true,
        "noFallthroughCasesInSwitch": true,
        "allowUnreachableCode": false,
        "outFile": "./public/js/app.js"
    },
    "files": ["public/js/core/application.ts",
        "public/js/services/auth-services.ts",
        "public/js/services/navigation-services.ts",
        "public/js/services/user-services.ts",
        "public/js/core/application-config.ts",
        "public/js/core/application-run.ts",
        "public/js/directives/navigation-bar/navigation-directive.ts",
        "public/js/controllers/global-controller.ts",
        "public/js/controllers/home-controller.ts",
        "public/js/controllers/register-controller.ts"
    ]
}